    "format-check": "prettier --check **/*.js"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.0.1",
    "@actions/io": "^1.1.0",
    "@martijnhols/actions-cache": "^1.1.3"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.31.1",
    "husky": "^4.3.8",
    "install": "^0.13.0",
    "prettier": "^2.3.0"
  }
}
