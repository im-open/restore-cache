{
  "name": "restore-cache",
  "version": "1.0.0",
  "description": "Restore a cache without saving it post-job ",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm ci && ncc build -o dist src/main.js && npm run format && git add dist/*",
